@media(max-width:767px){
  tab-component .slide-btn.flickity-nav {
    width: auto;
    height: auto;
  }
  tab-component .slide-btn.flickity-next {
    right: 30px;
  }
  tab-component .slide-btn.flickity-prev {
    left: 30px;
  }
  tab-component .slide-btn span {
    padding: 5px 10px;
    font-size: 40px;
    font-weight: 300;
    color: #acacac;
    display: inline-block;
    vertical-align: middle;
    line-height: 1;
  }
  slider-component.custom_slider{
    margin: 0 -16px !important;
  }
  tab-component .nav-link{
    padding-top:18px;
    padding-bottom:18px
  }
  .template-page-about .featured_tab{
    margin:10px 0 3rem
  }
  .banner_hero {
    height: 520px !important;
    padding-bottom: 0;
    border-radius: 5px;
    overflow: hidden;
  }
}  
