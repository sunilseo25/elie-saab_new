                          href="{% if block.settings.url != blank %}{{ block.settings.url }}{% else %}javascript:void(0);{% endif %}"
                          class="product_content text-center"
                        >
                          <span class="text-button-underline">{{ block.settings.text }}</span>
                        </a>
                      {% endif %}
                    </div>
                  </div>
                {% endif %}

