<script>
  $(document).ready(function () {
   $('.play-btn').on('click', function () {
      var videoSelector = $(this).data('target');
      var video = $(videoSelector).get(0);
      if (video) {
        video.play();
        video.setAttribute('controls', true); // Optional: show controls on play
        $(this).hide(); // Hide the play button
      }
    });
    function setUnderline() {
      var $activeTab = $(".about-tabs .nav-link.active");
      var $underline = $(".aboutprogress");
      if ($activeTab.length > 0) {
        if (window.innerWidth <= 768) {
        if($activeTab.html() == 'Legacy') {
          $underline.css({
          width: 50,
          left: $activeTab.offset().left - $(".about-tabs").offset().left,
        });
        } else if($activeTab.html() == 'About') {
          $underline.css({
          width: 50,
          left: $activeTab.offset().left - $(".about-tabs").offset().left,
          });
        } else {
          $underline.css({
          width: 130,
          left: $activeTab.offset().left - $(".about-tabs").offset().left,
        });
      }
        } else {
        $underline.css({
          width: $activeTab.outerWidth(),
          left: $activeTab.offset().left - $(".about-tabs").offset().left,
        });
        }
    }
    }
    setUnderline();
    $(".about-tabs .nav-link").on("shown.bs.tab", function () {
      let target = $(this).attr('data-bs-target');
      $('.tab-pane').removeClass('show active');
      $('.' + target).addClass('show active');

      setTimeout(function () {
        $('slider-component').each(function () {
          let flkty = Flickity.data(this);
          if (flkty) {
            flkty.resize(); // Force Flickity to update layout
          }
        });
      }, 100);
      setUnderline();
    });
    $(window).on("resize", function () {
      setUnderline();
    });
  });


const hashAlias = { about: 'tab1', eliesaabworld: 'tab3' };


$(function () {
  const friendly = window.location.hash.replace('#', '');
  const className = hashAlias[friendly];          // "tab1" or "tab3"
  if (className) {
    $('.about-tabs .nav-link[data-bs-target="' + className + '"]').tab('show');
  }
});


$('.about-tabs .nav-link').on('shown.bs.tab', function () {
  const className = $(this).attr('data-bs-target');      // "tab1"/"tab3"
  const newHash   = Object.keys(hashAlias)
                        .find(k => hashAlias[k] === className);
  if (newHash) history.replaceState(null, null, '#' + newHash);
});
</script>

