<div class="section-spacing{% if disable_top_spacing %} section-spacing--disable-top{% endif %}{% if disable_bottom_spacing %} section-spacing--disable-bottom{% endif %}">
  <div class="row">
    <div class="small-12 columns">
      <div class="tab-pane active tab1">
        <div class="banner_hero cover_img object-top {% if section.settings.mn1 != blank %}m-none{% endif %}" style="--padding-bottom:49%">
          {% if section.settings.bn1 != blank %}
            <img
              src="{{ section.settings.bn1 | img_url:'master' }}"
              width="{{ section.settings.bn1.width }}"
              height="{{ section.settings.bn1.height }}"
            >
          {%- else %}
            {{ 'lifestyle-2' | placeholder_svg_tag: 'placeholder_svg_tag' }}
          {%- endif %}
        </div>
        {% if section.settings.mn1 != blank %}
          <div class="banner_hero cover_img d-none" style="--padding-bottom:150%">
            <img
              src="{{ section.settings.mn1 | img_url:'master' }}"
              width="{{ section.settings.mn1.width }}"
              height="{{ section.settings.mn1.height }}"
            >
          </div>
        {%- endif %}
      </div>
      {% if false %}
      <div class="tab-pane tab2">
        <div class="cover_img object-top {% if section.settings.mn2 != blank %}m-none{% endif %}" style="--padding-bottom:49%">
          {% if section.settings.bn2 != blank %}
            <img
              src="{{ section.settings.bn2 | img_url:'master' }}"
              width="{{ section.settings.bn2.width }}"
              height="{{ section.settings.bn2.height }}"
            >
          {%- else %}
            {{ 'lifestyle-2' | placeholder_svg_tag: 'placeholder_svg_tag' }}
          {%- endif %}
        </div>
        {% if section.settings.mn2 != blank %}
          <div class="cover_img d-none" style="--padding-bottom:150%">
            <img
              src="{{ section.settings.mn2 | img_url:'master' }}"
              width="{{ section.settings.mn2.width }}"
              height="{{ section.settings.mn2.height }}"
            >
          </div>
        {%- endif %}
      </div>
      {% endif %}
      <div class="tab-pane tab3">
        <div class="cover_img object-top  {% if section.settings.mn3 != blank %}m-none{% endif %}" style="--padding-bottom:49%">
          {% if section.settings.video3 != blank %}
            {{
              section.settings.video3
              | video_tag:
                image_size: 'master',
                autoplay: true,
                controls: false,
                muted: true,
                loop: true,
                playsinline: true
            }}
          {% else %}
            {{ 'lifestyle-2' | placeholder_svg_tag: 'placeholder_svg_tag' }}
          {% endif %}
        </div>
        {% if section.settings.mn3 != blank %}
          <div class="cover_img d-none" style="--padding-bottom:150%">
            <img
              src="{{ section.settings.mn3 | img_url:'master' }}"
              width="{{ section.settings.mn3.width }}"
              height="{{ section.settings.mn3.height }}"
            >
          </div>
        {% endif %}
      </div>

      <div class="featured_tab">
        <tab-component class="nav nav-tabs about-tabs">
          <li class="nav-item carousel">
            <a class="nav-link active" data-bs-toggle="tab" data-bs-target="tab1" href="javascript:void(0);">
              {{- section.settings.tab1 -}}
            </a>
          </li>
          {% if false %}
          <li class="nav-item carousel">
            <a class="nav-link" data-bs-toggle="tab" data-bs-target="tab2" href="javascript:void(0);">
              {{- section.settings.tab2 -}}
            </a>
          </li>
          {% endif %}
          <li class="nav-item carousel">
            <a class="nav-link" data-bs-toggle="tab" data-bs-target="tab3" href="javascript:void(0);">
              {{- section.settings.tab3 -}}
            </a>
          </li>
          <div class="slide-btn flickity-nav flickity-prev" tabindex="0">
            <span>‹</span>
          </div>
          <div class="slide-btn flickity-nav flickity-next" tabindex="0">
            <span>›</span>
          </div>
        </tab-component>
        <div class="underline-progress">
          <div class="aboutprogress under_progress"></div>
        </div>
      </div>

      <div class="tab-pane active tab1" id="tab1">
        {% if section.settings.heading != blank %}
          <h2 class="tab-heading h5">
            <span>{{ section.settings.heading }}</span>
          </h2>
        {% endif %}
        {% if section.settings.content != blank or section.settings.content_image %}
          <div class="tab_content{% if section.settings.brand %} brand__logo{% endif %}">
            <div>
              {% if section.settings.content != blank %}
                <div class="rte">
                  <p>{{ section.settings.content }}</p>
                </div>
              {% endif %}
              {% if section.settings.content_image != blank %}
                <img
                  src="{{ section.settings.content_image | img_url:'master' }}"
                  height="{{ section.settings.content_image.height }}"
                  width="{{ section.settings.content_image.width }}"
                >
              {% endif %}
            </div>
            {% if section.settings.brand != blank %}
              <img
                class="brand_logo"
                src="{{ section.settings.brand | img_url:'master'}}"
                height="{{ section.settings.brand.height }}"
                width="{{ section.settings.brand.width }}"
              >
            {% endif %}
          </div>
        {% endif %}
        {% if first_tab_blocks.size > 0 %}
          {% liquid
            if first_tab_blocks.size > 4
              assign slide = 'slider-component'
            else
              assign slide = 'div'
            endif
          %}
          <{{ slide }} class="custom_slider carousel products row">
            {% for block in first_tab_blocks %}
              <div class="carousel_slide small-6 medium-6 large-3 columns">
                <div class="product-card-item">
                  <div class="cover_img" id="block-{{ block.id }}" style="--padding-bottom:155%">
                    {% if block.settings.video_hosted != blank %}
                      {{
                        block.settings.video_hosted
                        | video_tag:
                          image_size: 'master',
                          autoplay: block.settings.video_autoplay,
                          controls: false,
                          muted: true,
                          loop: true,
                          playsinline: true
                      }}
                    {% else %}
                      <img
                        src="{{ block.settings.image | img_url: 'master' }}"
                        width="{{ block.settings.image.width }}"
                        height="{{ block.settings.image.height }}"
                        alt=""
                      >
                    {% endif %}
                  </div>
                  {% if block.settings.video_hosted != blank and block.settings.video_autoplay == false %}
                    <button class="play-btn" data-target="#block-{{ block.id }} video">
                      <img width="30" height="30" src="{{ 'arrow-right.svg' | asset_url }}">
                    </button>
                  {% endif %}
                </div>
              </div>
            {% endfor %}
            {% if first_tab_blocks.size > 4 %}
              <div class="flickity-nav product-next flickity-prev" tabindex="0">
                <img width="30" height="30" src="{{ 'arrow-left.svg' | asset_url }}">
              </div>
              <div class="flickity-nav product-prev flickity-next" tabindex="0">
                <img width="30" height="30" src="{{ 'arrow-right.svg' | asset_url }}">
              </div>
            {% endif %}
          </{{ slide }}>
        {% endif %}

        {% if slider_first.size > 0 %}
