  assign disable_bottom_spacing = section.settings.disable_bottom_spacing

  assign first_tab_blocks = section.blocks | where: 'type', 'video'
  assign second_tab_blocks = section.blocks | where: 'type', 'video2'
  assign third_tab_blocks = section.blocks | where: 'type', 'video3'

  assign slider_first = section.blocks | where: 'type', 'slider'
  assign slider_second = section.blocks | where: 'type', 'slider2'
  assign slider_third = section.blocks | where: 'type', 'slider3'
%}
