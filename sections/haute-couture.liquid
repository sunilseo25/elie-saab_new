<script type="load">
  // Convert to slider on mobile
  function initializeMobileSlider() {
    if (window.innerWidth <= 768) {
      if (!$('.cstbtnss').hasClass('slick-initialized')) {
        $('.cstbtnss').slick({
          slidesToShow: 1,
          slidesToScroll: 1,
          arrows: true,
   prevArrow: '<i class="fa fa-chevron-left"></i>',
          nextArrow: '<i class="fa fa-chevron-right"></i>',
          dots: false,
          responsive: [
            {
              breakpoint: 480,
              settings: {
                slidesToShow: 1,
              },
            },
          ],
        });
      }
    } else {
      if ($('.cstbtnss').hasClass('slick-initialized')) {
        $('.cstbtnss').slick('unslick');
      }
    }
  }

  // Initialize slider on load and window resize
  initializeMobileSlider();
  window.addEventListener('resize', initializeMobileSlider);
  // Initialize Slick Slider for all carousels
  $('.carousel').slick({
  dots: false,
  infinite: true,
  speed: 500,
  slidesToShow: 3,
  slidesToScroll: 1,
  autoplay: false,
  autoplaySpeed: 2000,
  arrows: true,

  responsive: [
  {
  breakpoint: 1024,
  settings: {
  slidesToShow: 3
  }
  },
  {
  breakpoint: 600,
  settings: {
  slidesToShow: 1,centerMode: true,
  arrows: false,
  }
  },
   {
              breakpoint: 480,
              settings: {
                slidesToShow: 1,
             centerMode: true,
  arrows: false,
              },}
  ]
  });
</script>

<script>
          document.addEventListener("DOMContentLoaded", function () {
      const buttons = document.querySelectorAll(".tab-btn");
      const sections = document.querySelectorAll(".content-section");

      function highlightActiveButton() {
          let index = sections.length;

          while (--index && window.scrollY + 100 < sections[index].offsetTop) {}

          buttons.forEach((btn) => btn.classList.remove("active"));
          buttons[index].classList.add("active");
      }

      buttons.forEach((button) => {
          button.addEventListener("click", function () {
              const targetId = this.getAttribute("data-target");
              const targetSection = document.getElementById(targetId);

              if (targetSection) {
                  // Scroll to the target section
                  targetSection.scrollIntoView({ behavior: "smooth", block: "start" });

                  // Remove active class from all buttons and add to the clicked one
                  buttons.forEach((btn) => btn.classList.remove("active"));
                  this.classList.add("active");
              }
          });
      });

      window.addEventListener("scroll", highlightActiveButton);
  });
</script>

<script>
  const banner = document.querySelector('.cstbnr');
  const target = document.querySelector('.target-div');

  window.addEventListener('scroll', () => {
    const bannerBottom = banner.getBoundingClientRect().bottom;
    const targetRect = target.getBoundingClientRect();
    const windowHeight = window.innerHeight;

    const isBannerOut = bannerBottom <= 0;
    const isTargetVisible = targetRect.bottom > 0 && targetRect.top < windowHeight;

    if (isBannerOut && isTargetVisible) {
      target.classList.add('active-kp');
    } else {
      target.classList.remove('active-kp');
    }
  });
</script>
